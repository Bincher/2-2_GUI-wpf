<Window x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="MAIN" Height="400" Width="800" Loaded="Window_Loaded">
    <StackPanel>
        <TextBlock Text="This is a TextBlock" />
        <TextBox Text="안녕하세요" />
        <ProgressBar Value ="90" Height="20"/>
        <Image Source="images/image.png" Height="200"/>
        <Button Padding="10" Width="200" Height ="200">
            <Image Source="images/image.png" Width="250" Height="200"/>
        </Button>
        <ScrollViewer Height="300" Width="300" HorizontalScrollBarVisibility="Auto">
            <Image Source="images/image.png" Stretch="None"/>
        </ScrollViewer>
        <Viewbox Stretch="Fill" Height="200" Width="300">
            <Button Content="A"/>
        </Viewbox>
    </StackPanel>
</Window>
